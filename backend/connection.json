{
  "name": "my-network",
  "version": "1.0.0",
  "client": {
    "organization": "SSN_name",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        },
        "orderer": "300"
      }
    }
  },
  "channels": {
    "mychannel": {
      "orderers": [
        "orderer.example.com"
      ],
      "peers": {
        "localhost": {},
        "localhost2": {}
      }
    }
  },
  "organizations": {
    "Org1": {
      "mspid": "Org1MSP",
      "peers": [
        "localhost"
      ],
      "certificateAuthorities": [
        "ca.org1.example.com"
      ]
    },
    "Org2": {
      "mspid": "Org2MSP",
      "peers": [
        "localhost2"
      ],
      "certificateAuthorities": [
        "ca.org2.example.com"
      ]
    }
  },
  "orderers": {
    "orderer.example.com": {
      "url": "grpc://orderer.example.com:7050"
    }
  },
  "peers": {
    "localhost": {
      "url": "grpc://localhost:7051",
      "eventUrl": "grpc://localhost:7053"
    },
    "localhost2": {
      "url": "grpc://localhost:8051",
      "eventUrl": "grpc://localhost:8053"
    }
  },
  "certificateAuthorities": {
    "ca.org1.example.com": {
      "url": "http://192.168.1.101:7054",
      "caName": "ca.org1.example.com"
